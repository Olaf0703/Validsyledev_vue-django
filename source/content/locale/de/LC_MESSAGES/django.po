msgstr "Einloggen"

#: content/templates/accounts/login.html:17
#: content/templates/accounts/password_reset.html:26
#: content/templates/accounts/resend_activation_code.html:26
msgid "Login"
msgstr "Anmeldung"

#: content/templates/accounts/login.html:26
#: content/templates/accounts/password_reset.html:10
#: content/templates/accounts/password_reset_complete.html:10
#: content/templates/accounts/password_reset_confirm.html:10
