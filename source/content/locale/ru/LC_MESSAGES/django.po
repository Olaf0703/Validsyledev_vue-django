msgstr "Английский"

msgid "Spanish"
msgstr "Испанский"

msgid "French"
msgstr "Французский"

msgid "Deutsch"
msgstr "Немецкий"

msgid "Ukrainian"
msgstr "Украинский"

msgid "Russian"
msgstr "Русский"

msgid "Simplified Chinese"
msgstr "Китайский (упрощённый)"

